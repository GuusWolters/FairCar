{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "backend",
  "main": "src/index.ts",
  "compatibility_date": "2025-03-28",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "logs": {
      "enabled": true,
    },
  },
  "env": {
    "production": {
      "vars": {
        "NODE_ENV": "production",
      },
    },
    "development": {
      "vars": {
        "NODE_ENV": "development",
      },
    },
  },
}
